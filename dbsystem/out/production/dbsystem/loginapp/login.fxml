<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="348.0" prefWidth="495.0" xmlns="http://javafx.com/javafx/10.0.2-internal"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="loginapp.LoginController">
    <children>
        <Label layoutX="48.0" layoutY="38.0" text="Database: "/>
        <Label fx:id="dbstatus" layoutX="117.0" layoutY="38.0" prefHeight="15.0" prefWidth="118.0" textFill="#00ff04"/>
        <TextField fx:id="username" layoutX="184.0" layoutY="100.0" promptText="username"/>
        <Label layoutX="102.0" layoutY="105.0" prefHeight="15.0" prefWidth="69.0" text="Username"/>
        <Label layoutX="105.0" layoutY="151.0" prefHeight="15.0" prefWidth="63.0" text="Password"/>
        <ComboBox fx:id="combobox" layoutX="184.0" layoutY="182.0" prefHeight="25.0" prefWidth="164.0"
                  promptText="Admin/Student"/>
        <Button fx:id="loginbutton" defaultButton="true" layoutX="234.0" layoutY="232.0" mnemonicParsing="false"
                onAction="#Login" text="Login" textAlignment="CENTER" textFill="#0a0a0a"
                textOverrun="CENTER_WORD_ELLIPSIS">
            <font>
                <Font name="Noto Sans Armenian Regular" size="14.0"/>
            </font>
        </Button>
        <Label fx:id="credentials" layoutX="48.0" layoutY="241.0" prefHeight="15.0" prefWidth="144.0"
               textFill="#f20000"/>
        <PasswordField fx:id="passwrd" layoutX="184.0" layoutY="146.0" promptText="password"/>
    </children>
</AnchorPane>
